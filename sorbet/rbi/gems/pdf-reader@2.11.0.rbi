  def end_inline_image(*args); end

  # source://pdf-reader//lib/pdf/reader/validating_receiver.rb#63
  sig { params(args: T.untyped).void }
  def end_text_object(*args); end

  # Form XObject Operators
  #
  # source://pdf-reader//lib/pdf/reader/validating_receiver.rb#211
